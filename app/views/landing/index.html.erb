<center><%= button_to 'Create New User', '/register', method: :get %></center>

<center><p>Existing Users:</p></center>
<center><% @users.each do |user| %>
  <p><%= link_to "#{user.name}'s Dashboard", user_dashboard_path(user) %></p>
  <% end %></center>

<div class="login">
  <% if session[:user_id] %>
    <%= button_to 'Log Out', '/login', method: :delete %>
  <% else %>
    <%= button_to 'Log In', '/login', method: :get %>
  <% end %>
</div>
